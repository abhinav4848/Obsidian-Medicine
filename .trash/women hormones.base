views:
  - type: table
    name: Table
    filters:
      and:
        - file.tags.contains("gynae")
        - or:
            - file.name.contains("cocp")
            - file.name.contains("hrt")
            - file.name.contains("menopause")
            - file.name.contains("contraceptive")
    order:
      - file.name
      - file.tags
    sort:
      - property: file.tags
        direction: ASC
      - property: file.name
        direction: ASC
